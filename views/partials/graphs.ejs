        function drawlineHifa() {
            var data = google.visualization.arrayToDataTable([
                ["Element", "Density", { role: "style" }],
                ["No tax", l75_h, "red"],
                ["VAT", l500_h, "red"],
                ["VAT ,sales tax and customs", h_h, "red"],

            ]);

            var view = new google.visualization.DataView(data);
            view.setColumns([0, 1,
                {
                    calc: "stringify",
                    sourceColumn: 1,
                    type: "string",
                    role: "annotation"
                },
                2]);

            var options = {
                title: "Hifa district \n tax destribution",
                width: 280,
                height: 280,
                bar: { groupWidth: "90%" },
                legend: { position: "none" },
            };
            var chart = new google.visualization.ColumnChart(document.getElementById("line_Chart_hifa"));
            chart.draw(view, options);
        }

