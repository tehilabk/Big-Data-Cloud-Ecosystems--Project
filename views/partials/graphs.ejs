        // Callback that draws the pie chart for Center district packages' size distribution
        function drawCenterChart() {

            // Create the data table for Center district
            var data = new google.visualization.DataTable();
            data.addColumn('string', 'size');
            data.addColumn('number', 'packages');
            data.addRows([
                ['S', s_c],
                ['M', m_c],
                ['L', l_c]
            ]);

            // Set options for chart
            var options = {
                title: 'Center district:\n Packages size distribution',
                is3D: true,
                width: 280,
                height: 280
            };

            // Instantiate and draw the chart
            var chart = new google.visualization.PieChart(document.getElementById('center_size_dist'));
            chart.draw(data, options);
        }

