<!DOCTYPE html>
<html >

<head>
  <%- include("../partials/headers") %>
    <!--ariel ws-->
    <script src="/socket.io/socket.io.js"></script>
    <script>
      var socket = io.connect();
      function setWs() {
        socket.on('newdata', function (msg) {
          var element = document.getElementById(msg.districtId);
          element.innerText = msg.value;
        })
      }
    </script>
    <!--ariel ws -->
</head>

  <body class="" onload="setWs()" style="background-color:rgb(201, 178, 177);">
    <div class="wrapper ">
      <%- include("../partials/sidebar") %>
        <div class="main-panel">
            <div class="content">
              <h2 style= "color: rgb(73, 76, 78);">~information about the packages that has been sent~</h2>
              <div class="container-fluid">
                <div class="row">
                  <!--start-->
                  <% cards.forEach(function(card) { %>
                    <%- include("../partials/card",card) %>
                      <% }); %>
                        <!--end-->
                </div>
                <div class="row">
                  <%- include("../partials/graphs") %>
                  
                 
                </div>
                
              </div>
            </div>
           
        </div>
    </div>
    <%- include("../partials/dashcode") %>  
  </body>

</html>